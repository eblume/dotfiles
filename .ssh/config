Include /Users/erich/.colima/ssh_config

IgnoreUnknown UseKeychain  # Mac OS specific thing
Include ~/.ssh/1password.conf # 1password identity agent

Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519


